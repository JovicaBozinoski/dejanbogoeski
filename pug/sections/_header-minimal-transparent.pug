header.page-header.page-header-absolute.page-header-dark
	//- RD Navbar
	.rd-navbar-wrap
		nav.rd-navbar.rd-navbar-default.rd-navbar-wide.rd-navbar-minimal.rd-navbar-minimal-dark.rd-navbar-minimal-transparent(
			data-layout=                'rd-navbar-fixed'
			data-sm-layout=             'rd-navbar-fixed'
			data-md-layout=             'rd-navbar-fixed'
			data-md-device-layout=      'rd-navbar-fixed'
			data-lg-layout=             'rd-navbar-static'
			data-lg-device-layout=      'rd-navbar-static'
			data-xl-layout=             'rd-navbar-static'
			data-xl-device-layout=      'rd-navbar-static'
			data-lg-stick-up-offset=    '46px'
			data-xl-stick-up-offset=    '46px'
			data-xxl-stick-up-offset=   '46px'
			data-lg-stick-up=           'true'
			data-xl-stick-up=           'true'
			data-xxl-stick-up=          'true'
		)
			.rd-navbar-inner
				//- RD Navbar Panel
				.rd-navbar-panel
					//- RD Navbar Toggle
					button.rd-navbar-toggle(data-rd-navbar-toggle='.rd-navbar-nav-wrap')
						span

				.rd-navbar-minimal-inner
					.rd-navbar-minimal-left
						.rd-navbar-nav-wrap
							.rd-navbar-nav-scroll-holder
								a.brand(href="index.html")
									img(src="images/logo-dark-150x150.png" width="150" height="150" alt="")
								+navigation( menu )

					.rd-navbar-minimal-right
						a.icon.icon-sm.mdi.mdi-cart-outline(href="checkout.html")
							span 2
						//RD Navbar Search
						.rd-navbar-search
							a.rd-navbar-search-toggle(data-rd-navbar-toggle=".rd-navbar-search, .rd-navbar-inner" href="#")
								span
							form.rd-search(action="search-results.html" data-search-live="rd-search-results-live" method="GET")
								div.form-wrap
									label.form-label(for="rd-navbar-search-form-input" class="form-label") Search...
									input(class="rd-navbar-search-form-input form-input" id="rd-navbar-search-form-input" type="text" name="s" autocomplete="off")
									#rd-search-results-live.rd-search-results-live


				.rd-navbar-minimal-inner-bottom
					a.rd-navbar-collapse-toggle(data-rd-navbar-toggle=".rd-navbar-minimal-inner-bottom" href="#")
						span
					.contact-info
						.contact-info-left
							.unit.unit-12.align-items-center
								.unit-left
									span.icon.icon-sm.icon-circle.icon-bordered.icon-white.mdi.mdi-map-marker
								.unit-body
									p
										a(href="contacts.html") 2130 Fulton Street, San #[br.d-none.d-xxl-inline-block] Diego, CA 94117-1080 USA
						.contact-info-right
							.unit.unit-12.align-items-center.flex-row-reverse.justify-content-end.flex-lg-row
								.unit-body
									p Booking Table
									a(href="tel:#") 1-800-1234-567
								.unit-right
									span.icon.icon-sm.icon-circle.icon-bordered.icon-white.mdi.mdi-phone